//Aqui o código configura um perfil de inspeção em um IDE para detectar código duplicado em Java, definindo o nível de aviso como "WEAK WARNING" e ativando essa verificação por padrão.
<component name="InspectionProjectProfileManager">
  <profile version="1.0">
    <option name="myName" value="Project Default" />
    <inspection_tool class="DuplicatedCode" enabled="true" level="WEAK WARNING" enabled_by_default="true">
      <Languages>
        <language minSize="217" name="Java" />
      </Languages>
    </inspection_tool>
  </profile>
</component>
